<?xml version="1.0" encoding="UTF-8"?><!-- Use this file to deploy some handlers/chains 
	and services --><!-- Two ways to do this: --><!-- java org.apache.axis.client.AdminClient 
	deploy.wsdd --><!-- after the axis server is running --><!-- or --><!-- java org.apache.axis.utils.Admin 
	client|server deploy.wsdd --><!-- from the same directory that the 
	Axis engine runs -->
<deployment xmlns="http://xml.apache.org/axis/wsdd/"
	xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

	<!-- Services from ProviderManagerService WSDL service -->

	<service name="ProviderManager" provider="java:RPC" style="wrapped"
		use="literal">
		<parameter name="wsdlTargetNamespace" value="http://manager.fiap.com.br" />
		<parameter name="wsdlServiceElement" value="ProviderManagerService" />
		<parameter name="schemaQualified" value="http://manager.fiap.com.br" />
		<parameter name="wsdlServicePort" value="ProviderManager" />
		<parameter name="className" value="br.com.fiap.manager.ProviderManager" />
		<parameter name="wsdlPortType" value="ProviderManager" />
		<parameter name="typeMappingVersion" value="1.2" />
		<operation xmlns:operNS="http://manager.fiap.com.br"
			xmlns:retNS="http://manager.fiap.com.br" xmlns:rtns="http://www.w3.org/2001/XMLSchema"
			name="cadastrarPessoa" qname="operNS:cadastrarPessoa" returnQName="retNS:cadastrarPessoaReturn"
			returnType="rtns:string" soapAction="">
			<parameter xmlns:pns="http://manager.fiap.com.br"
				xmlns:tns="http://www.w3.org/2001/XMLSchema" qname="pns:cpf" type="tns:long" />
			<parameter xmlns:pns="http://manager.fiap.com.br"
				xmlns:tns="http://www.w3.org/2001/XMLSchema" qname="pns:nome" type="tns:string" />
			<parameter xmlns:pns="http://manager.fiap.com.br"
				xmlns:tns="http://www.w3.org/2001/XMLSchema" qname="pns:idade" type="tns:int" />
			<parameter xmlns:pns="http://manager.fiap.com.br"
				xmlns:tns="http://www.w3.org/2001/XMLSchema" qname="pns:sexo" type="tns:string" />
		</operation>
		<parameter name="allowedMethods" value="cadastrarPessoa" />

	</service>
</deployment>